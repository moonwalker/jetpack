{
  "presets": [
    ["env", {
      "modules": false
    }],
    "stage-2",
    "react"
  ],
  "plugins": [
    "react-require",
    "transform-class-properties",
    "transform-decorators-legacy",
    ["transform-runtime", {
      "helpers": false,
      "polyfill": false,
      "regenerator": true
    }]
  ],
  "env": {
    "test": {
      "presets": [
        "env",
        "stage-2",
        "react"
      ],
      "plugins": [
        "react-require",
        "transform-class-properties",
        "transform-decorators-legacy"
      ]
    }
  }
}
